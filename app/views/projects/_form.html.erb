<%= form_for @project, :html => { :multipart => true } do |f| %>

  <div class="form-group">
    <%= f.label :projtitle, :class => 'control-label col-md-2' %>
    <div class="col-md-6">
      <%= f.text_field :projtitle, :class => 'text_field form-control' %>
    </div>
  </div>

  <div class="form-group">
    <%= f.label :projdesc, :class => 'control-label col-md-2' %>
    <div class="col-md-6">
      <%= f.text_area :projdesc, :class => 'text_area form-control' %>
    </div>
  </div>

  <div class="form-group">
    <div class='col-md-offset-2 col-md-8'>
      <%= f.submit nil, :class => 'btn btn-primary' %>
      <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                  projects_path, :class => 'btn btn-default' %>
    </div>
  </div>

    <%= f.label :Photo %>
    <%= f.file_field :photo %>
    
    <%= f.label :AttachFile %>
    <%= f.file_field :attachfile %>

<% end %>
